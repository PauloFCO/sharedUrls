      "matches": ["https://*.youtube.com/*"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/ext-icon.png",
      "24": "assets/ext-icon.png",
      "32": "assets/ext-icon.png"
    },
    "default_title": "React extension",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/ext-icon.png",
    "48": "assets/ext-icon-48.png",
    "128": "assets/ext-icon-128.png"
  },
  "manifest_version": 3
