{
  "name": "Easy cap",
  "version": "0.2.0",
  "description": "TFG Extension",
  "permissions": ["scripting", "storage", "tabs", "webNavigation"],
  "host_permissions": [
"/*",
"https://loud-states-relax.loca.lt/*",
"/*",
"https://eight-pans-hide.loca.lt/*",
"https://light-camels-stay.loca.lt/*",
"https://mighty-bars-drive.loca.lt/*",
"https://petite-guests-feel.loca.lt/*",
"https://legal-islands-leave.loca.lt/*",
"https://late-hairs-itch.loca.lt/*",
"https://icy-weeks-bet.loca.lt/*",
"https://violet-guests-agree.loca.lt/*",
"https://hot-poets-nail.loca.lt/*",
"https://wicked-worms-clean.loca.lt/*",
"https://quick-islands-punch.loca.lt/*",
"https://nine-beans-stick.loca.lt/*",
      "https://*.youtube.com/*",
    ],
  "background": {
